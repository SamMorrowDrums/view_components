{
  "version": 2,
  "name": "primer-viewcomponents",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {"distDir": "public"}
    }
  ],
  "rewrites": [{"source": "/view-components(/.*)?", "destination": "/$1"}],
  "redirects": [{"source": "/", "destination": "/view-components"}],
  "github": {
    "enabled": false
  }
}
